
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RequestExample</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.0.js"></script>
    <script src="~/Scripts/popper.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
</head>
<body>
    <div>
        <nav class="navbar navbar-expand-sm bg-warning navbar-light">

            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Product">Products</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Contact">Contact Us</a>
                </li>
            </ul>
        </nav>

        <div class="container-fluid">
            <table class="table">
                <tr>
                    <td>Url</td>
                    <td>@ViewBag.Url</td>
                </tr>
                
                <tr>
                    <td>PhysicalApplicationPath</td>
                    <td>@ViewBag.PhysicalApplicationPath</td>
                </tr>
                <tr>
                    <td>Path</td>
                    <td>@ViewBag.Path</td>
                </tr>
                <tr>
                    <td>BrowserType</td>
                    <td>@ViewBag.BrowserType</td>
                </tr>
                <tr>
                    <td>QueryString</td>
                    <td>@ViewBag.QueryString</td>
                </tr>
                <tr>
                    <td>Headers</td>
                    <td>@ViewBag.Headers</td>
                </tr>
                <tr>
                    <td>HttpMethod</td>
                    <td>@ViewBag.HttpMethod</td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>
