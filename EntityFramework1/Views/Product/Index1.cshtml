@model List<EntityFramework1.Models.product>

@{
    ViewBag.Title = "Index1";
    Layout = "~/Views/Shared/Layout1.cshtml";
}
<h2>Products</h2>

<form class="form-inline">
    <label for="search">Search</label>
    <input type="search" class="form-control" id="search" name="search" value="@ViewBag.s" />
    <button type="submit" class="btn-btn-warning">Go</button>
</form>
<table class="table">
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
        <th>Product Price</th>
        <th>Date of purchase</th>
        <th>Product status</th>
        <th>Category id</th>
        <th>Brand id</th>
        <th>Active</th>




    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.pid</td>
            <td>@item.pname</td>
            <td>@item.price</td>
            <td>@item.dop</td>
            <td>@item.astatus</td>
            <td>@item.cid</td>
            <td>@item.bid</td>
            <td>@item.active</td>
        </tr>
    }
</table>



