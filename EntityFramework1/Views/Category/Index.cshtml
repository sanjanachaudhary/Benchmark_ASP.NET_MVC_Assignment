@model List<EntityFramework1.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layout1.cshtml";
}

<h2>Category</h2>
<table class="table">
    <tr>
        <th>Ctegory Id</th>
        <th>Category Name</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.cid</td>
                    <td>@item.cname</td>
                </tr>
            }
</table>

